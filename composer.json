{
    "name": "queicherius/gw2-api",
    "description": "Proxy-layer for the official GW2 item, tradingpost and render API",
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.0.*",
        "vlucas/phpdotenv": "~1.0",
        "predis/predis": "~1.0",
        "illuminate/redis": "~5.0",
        "intervention/image": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "itsgoingd/clockwork": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
